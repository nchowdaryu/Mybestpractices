import org.springframework.context.ApplicationEvent;

/**
 * HttpSessionInvalidatedEvent is published by a listener implementing 
 * HttpSessionListener.
*/
public class HttpSessionInvalidatedEvent extends ApplicationEvent {

    /**
     * 
     */
    private static final long serialVersionUID = -3314417662493212940L;

    /**
     * @param source
     */
    public HttpSessionInvalidatedEvent(Object source) {
        super(source);
    }
}
